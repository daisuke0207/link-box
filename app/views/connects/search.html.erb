<%= render "shared/header" %>
<div class="connect-list">
  <% @search_connects.each do |connect| %>
    <div class="connect-container">
      <a class = "image-link" href= "<%= text_url_to_link(connect.link) %>" target=_blank, rel="noopener noreferrer" ><img src="http://favicon.hatena.ne.jp/?url=<%= text_url_to_link(connect.link) %>" style="width:50px; height:50px"></a>
      <%= link_to connect.title, text_url_to_link(connect.link), class: "link-title", target: :_blank, rel:"noopener noreferrer" %>
    </div>
  <% end %>
</div>